{
  "name": "autogents",
  "version": "0.8.1",
  "description": "autogents",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "Tim Bavaro",
    "email": "tbavaro@gmail.com",
    "url": "http://timba.me"
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "bin": {
    "generate-validators": "lib/apps/GenerateValidatorsApp.js"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src}/**/*.ts\" es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && echo skipping npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch",
    "push": "./node_modules/.bin/tsdevtools deploy"
  },
  "dependencies": {
    "@types/node": "^11.13.0",
    "@types/prettier": "^1.16.1",
    "prettier": "^1.16.4",
    "tsvalidators": "github:tbavaro/tsvalidators#dist",
    "typescript": "^3.4.2",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/yargs": "^11.1.2",
    "coveralls": "^3.0.3",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^3.2.0",
    "tsdevtools": "github:tbavaro/tsdevtools#dist",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
